{
  "builds": [
    {
      "src": "sistema_vendas/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "sistema_vendas/wsgi.py"
    }
  ]
}
